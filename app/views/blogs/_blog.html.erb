<div id="<%= dom_id blog %>">
  <p>
    <strong>Title:</strong>
    <%= blog.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= blog.content %>
  </p>

  <p>
    <strong>Likes:</strong>
    <%= blog.likes %>
  </p>

  <p>
    <strong>Dislikes:</strong>
    <%= blog.dislikes %>
  </p>

  <h2>Files:</h2>
  <ul>
    <% blog.blogimages.each do |file| %>
    <%=image_tag file,height:"200",width:"200"%>
    <%end%>
  </ul>
<h2>Comments:</h2>
<% blog.comments.each do |comment| %>
  <%= comment.id%><br>
  <%= comment.content%><br>
<% end %>
</div>
