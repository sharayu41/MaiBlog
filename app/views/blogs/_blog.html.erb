<div id="<%= dom_id blog %>">
  <p>
    <strong>Title:</strong>
    <%= blog.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= blog.content %>
  </p>

  <p>
    <strong>Likes:</strong>
    <%= blog.likes %>
  </p>

  <p>
    <strong>Dislikes:</strong>
    <%= blog.dislikes %>
  </p>

  <h2>Files:</h2>
  <ul>
  <%= blog.blogimages.each do |file| %>
<%= file%>
<%end%>
  </ul>


</div>
